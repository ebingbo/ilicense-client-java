{
  "groups": [
    {
      "name": "license",
      "type": "com.ibingbo.sdk.license.config.LicenseProperties",
      "sourceType": "com.ibingbo.sdk.license.config.LicenseProperties"
    }
  ],
  "properties": [
    {
      "name": "license.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用License验证",
      "defaultValue": true
    },
    {
      "name": "license.public-key",
      "type": "java.lang.String",
      "description": "公钥（Base64编码）"
    },
    {
      "name": "license.storage-path",
      "type": "java.lang.String",
      "description": "License存储路径"
    },
    {
      "name": "license.validate-on-startup",
      "type": "java.lang.Boolean",
      "description": "是否在启动时验证License",
      "defaultValue": false
    },
    {
      "name": "license.allow-start-when-expired",
      "type": "java.lang.Boolean",
      "description": "License过期后是否允许启动",
      "defaultValue": true
    },
    {
      "name": "license.enable-api",
      "type": "java.lang.Boolean",
      "description": "是否启用REST API",
      "defaultValue": true
    },
    {
      "name": "license.api-prefix",
      "type": "java.lang.String",
      "description": "REST API路径前缀",
      "defaultValue": "/api/license"
    },
    {
      "name": "license.expiry-warning-days",
      "type": "java.lang.Integer",
      "description": "过期提醒天数",
      "defaultValue": 30
    },
    {
      "name": "license.check-interval",
      "type": "java.time.Duration",
      "description": "License检查间隔",
      "defaultValue": "1h"
    },
    {
      "name": "license.enable-default-listener",
      "type": "java.lang.Boolean",
      "description": "是否启用默认事件监听器",
      "defaultValue": false
    },
    {
      "name": "license.enable-scheduled-check",
      "type": "java.lang.Boolean",
      "description": "是否启用定时License检查",
      "defaultValue": false
    }
  ]
}